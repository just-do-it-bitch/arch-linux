! Window opacity
! Opacity was removed in favor of ARGB
! rofi.opacity: 90

! State:           'bg',       'fg',    'bgalt',     'hlbg',     'hlfg'
rofi.color-normal: #88161616,  #5c5dad,  #88000000,  #88000000,  #c9c925
rofi.color-urgent: #88161616,  #ad3737,  #88000000,  #88000000,  #fdf6e3
